{
    "name": "yokuru/laravel-chatwork",
    "description": "Laravel notification driver for Chatwork.",
    "keywords": [
        "laravel",
        "chatwork",
        "notification"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yokuru",
            "email": "takashi.yokubo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Yokuru\\Chatwork\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yokuru\\ChatworkTest\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yokuru\\Chatwork\\ServiceProvider"
            ]
        }
    }
}
